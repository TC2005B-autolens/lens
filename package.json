{
  "name": "lens",
  "module": "src/app.ts",
  "type": "module",
  "scripts": {
    "start": "bun src/app.ts",
    "dev": "bun --watch src/app.ts",
    "dev-docker-watch": "docker compose -f compose.yaml -f compose.dev.yaml up --build --watch && bun docker-clean",
    "dev-redis": "docker compose -f compose.yaml -f compose.dev.yaml up -d redis",
    "dev-redis-cli": "docker compose -f compose.yaml -f compose.dev.yaml exec -it redis redis-cli",
    "docker-clean": "docker compose down"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/cors": "^2.8.17",
    "@types/dockerode": "^3.3.28",
    "@types/express": "^4.17.21",
    "@types/tar-stream": "^3.1.3",
    "pino-pretty": "^11.0.0"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@redis/json": "^1.0.6",
    "cors": "^2.8.5",
    "dockerode": "^4.0.2",
    "express": "^4.19.2",
    "express-async-errors": "^3.1.1",
    "http-errors": "^2.0.0",
    "nanoid": "^5.0.7",
    "pino": "^8.20.0",
    "pino-http": "^9.0.0",
    "redis": "^4.6.13",
    "tar-stream": "^3.1.7",
    "zod": "^3.22.4"
  }
}