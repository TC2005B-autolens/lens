{
  "name": "lens",
  "module": "src/app.ts",
  "type": "module",
  "scripts": {
    "start": "bun src/app.ts",
    "dev": "bun --watch src/app.ts",
    "dev-docker-watch": "docker compose -f compose.yaml -f compose.dev.yaml up --build --watch",
    "dev-redis": "docker compose -f compose.yaml -f compose.dev.yaml up -d redis",
    "dev-redis-cli": "docker compose -f compose.yaml -f compose.dev.yaml exec --it --rm redis redis-cli"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "pino-pretty": "^11.0.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@redis/json": "^1.0.6",
    "cors": "^2.8.5",
    "crockford-base32": "^2.0.0",
    "express": "^4.19.2",
    "express-async-errors": "^3.1.1",
    "http-errors": "^2.0.0",
    "nanoid": "^5.0.7",
    "pino": "^8.20.0",
    "pino-http": "^9.0.0",
    "redis": "^4.6.13",
    "uuid": "^9.0.1"
  }
}